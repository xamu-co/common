{
  "name": "@open-xamu-co/common-enums",
  "version": "0.0.1",
  "description": "Xamu shared enums",
  "author": "@xamu-co",
  "main": "lib/index.js",
  "exports": {
    ".": {
      "node": {
        "module": "./lib/esm/index.js",
        "require": "./lib/index.js",
        "import": "./wrapper.mjs"
      },
      "default": "./lib/esm/index.js"
    },
    "./package.json": "./package.json"
  },
  "module": "./lib/esm/index.js",
  "types": "lib/esm/index.d.ts",
  "files": [
    "CHANGELOG.md",
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xamu-co/common.git"
  },
  "keywords": [
    "ts",
    "js",
    "enum"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/xamu-co/common/issues"
  },
  "homepage": "https://github.com/xamu-co/common#readme",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "scripts": {
    "build:esm": "tsc",
    "build:cjs": "babel src --out-dir lib --extensions \".ts,.tsx\" --source-maps inline",
    "build": "rimraf lib && npm run build:esm && npm run build:cjs",
    "prepare": "npm run build"
  },
  "devDependencies": {
    "@babel/cli": "^7.22.6",
    "@babel/core": "^7.22.8",
    "@babel/preset-env": "^7.22.7",
    "@babel/preset-typescript": "^7.22.5"
  },
  "browserslist": [
    "defaults"
  ]
}